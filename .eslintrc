{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "jsx-quotes":"off",
    "react/prop-types":0,
    "react/display-name":0,
    "no-undef":"warn",
    "no-unused-vars":"warn",
    "no-throw-literal":0
  },
  
  "env": {
    "browser":true,
    "es6": true,
    "node": true
  },
 

  "globals": {
    "param": true,
    "returns": true
  },
  "plugins": [
    "node",
    "react"
  ]
}