{

  "parser": "babel-eslint",

  "rules": {
    "jsx-quotes":"off",
    "react/prop-types": 0,
    "react/display-name":0,
    "no-undef":"warn",
    "no-unused-vars":"warn",
    "no-throw-literal":"off"
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}